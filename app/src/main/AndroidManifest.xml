<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.swift.android.musicsitefragment">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".SwiftApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewuserActivity" />
        <activity android:name=".IntroductionActivity" />
        <activity android:name=".Battle1Activity" />
        <activity android:name=".Battle2Activity" />
        <activity android:name=".Battle3Activity" />
        <activity android:name=".Battle4Activity" />
        <activity android:name=".Battle5Activity" />
        <activity android:name=".ClearActivity" />
        <activity android:name=".Fragment1Activity" />
        <activity android:name=".Fragment2Activity" />
        <activity android:name=".Fragment3Activity" />
        <activity android:name=".Fragment4Activity" />
        <activity android:name=".Fragment5Activity" />
        <activity android:name=".BackgroundMusicService" />

        <service
            android:name=".BackgroundMusicService"
            android:enabled="true"
            android:exported="false" />

        <activity android:name=".SwiftApplication"></activity>
    </application>

</manifest>